{
  "manifest_version": 2,
  "name": "chrome-screen-stream-extension",
  "version": "0.10",
  "background": {
    "scripts": ["extension.js"]
  },
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://*.example.com/*", "https://scvv-twi.dev.instance.scopicsoftware.com/"]
  },
  "permissions": [
    "desktopCapture",
    "tabs"
  ]
}